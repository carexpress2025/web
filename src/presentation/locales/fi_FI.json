  "pages": {
    "terms": {
      "section1": {
        "title": "1. Ehtojen hyväksyminen",
        "content": "Kun käytät alustaa, hyväksyt nämä käyttöehdot. Jos et hyväksy jotain näistä ehdoista, älä käytä palvelujamme."
      },
      "section2": {
        "title": "2. Rajoitettu pääsy",
        "content": "Alustamme on rajoitettu ja se on tarkoitettu vain vahvistetuille käyttäjille. Luvaton pääsy tai yritykset kiertää turvajärjestelmiämme johtavat asianmukaisiin oikeudellisiin toimiin."
      },
      "section3": {
        "title": "3. Palvelut ja maksut",
        "content": "Palvelumme helpottaa viestintää ajoneuvojen ostajien ja myyjien välillä automatisoidun viestintäjärjestelmän avulla. Jokainen lähetetty viesti maksaa 0,05 € (viisi senttiä euroa)."
      },
      "section4": {
        "title": "4. Vastuuvapauslauseke",
        "content": "Alustamme toimii vain välikätenä ostajien ja myyjien välisessä viestinnässä. Me emme omista lueteltuja ajoneuvoja emmekä ole vastuussa seuraavista:",
        "list": {
          "item1": "Myyjien antamien tietojen tarkkuus",
          "item2": "Luetteloitujen ajoneuvojen kunto",
          "item3": "Osapuolten väliset neuvottelut tai kaupat",
          "item4": "Ajoneuvojen tekniset tai mekaaniset ongelmat",
          "item5": "Ostajien ja myyjien väliset riidat tai konfliktit"
        }
      },
      "section5": {
        "title": "5. WhatsApp-botin käyttö",
        "content": "Alustamme käyttää automatisoitua WhatsApp-bottia, joka mahdollistaa:",
        "list": {
          "item1": "Samanaikaisen viestinnän useiden myyjien kanssa",
          "item2": "Ajoneuvo- ja myyjätietojen automatisoidun keräämisen",
          "item3": "Reaaliaikaisten keskustelujen aloittamisen"
        },
        "extra": "Käyttäjät ovat vastuussa tämän työkalun asianmukaisesta käytöstä ja heidän on noudatettava WhatsAppin käyttöpolitiikkaa."
      },
      "section6": {
        "title": "6. Tietosuoja",
        "content": "Kaikki alustamme keräämät ja käsittelemät tiedot ovat meidän tietosuojakäytännön alaisia. Luvaton tai epäasianmukainen tietojen käyttö johtaa oikeudellisiin seuraamuksiin."
      },
      "section7": {
        "title": "7. Ehtojen muutokset",
        "content": "Pidätämme oikeuden muuttaa näitä ehtoja milloin tahansa. Käyttäjän vastuulla on tarkistaa mahdolliset päivitykset säännöllisesti."
      }
    },
    "privacy": {
      "title": "Tietosuojakäytäntö",
      "section1": {
        "title": "1. Tietojen kerääminen",
        "content": "Alustamme kerää ja käsittelee palvelun toiminnan kannalta tarvittavat tiedot, mukaan lukien:",
        "list": {
          "item1": "Käyttäjän rekisteröinti- ja vahvistustiedot",
          "item2": "Ilmoitettujen ajoneuvojen tiedot",
          "item3": "Viestihistoria ja vuorovaikutukset",
          "item4": "Myyjien yhteystiedot"
        }
      },
      "section2": {
        "title": "2. Tietojen käyttö",
        "content": "Kerättyjä tietoja käytetään seuraaviin tarkoituksiin:",
        "list": {
          "item1": "Viestinnän helpottaminen ostajien ja myyjien välillä",
          "item2": "Käyttäjäkokemuksen parantaminen alustalla",
          "item3": "Palvelun turvallisuuden ja eheyden varmistaminen",
          "item4": "Maksujen ja maksujen käsittely",
          "item5": "Lakivelvoitteiden täyttäminen"
        }
      },
      "section3": {
        "title": "3. Tietosuoja",
        "content": "Olemme ottaneet käyttöön teknisiä ja organisatorisia toimenpiteitä suojellaksemme tietojasi, kuten:",
        "list": {
          "item1": "Herkän tiedon salaus",
          "item2": "Rajoitettu pääsynhallinta",
          "item3": "Turvallisuuden valvonta",
          "item4": "Säännölliset varmuuskopiot"
        }
      },
      "section4": {
        "title": "4. Tietojen jakaminen",
        "content": "Tietojasi voidaan jakaa vain seuraavissa olosuhteissa:",
        "list": {
          "item1": "Ostajien ja myyjien välillä neuvottelujen helpottamiseksi",
          "item2": "Palveluntarjoajien kanssa, jotka ovat tarpeen alustan toiminnan kannalta",
          "item3": "Lain tai oikeuden määräyksestä"
        }
      },
      "section5": {
        "title": "5. Oikeutesi",
        "content": "Sinulla on oikeus:",
        "list": {
          "item1": "Pääsy henkilökohtaisiin tietoihisi",
          "item2": "Pyytää virheellisten tietojen oikaisua",
          "item3": "Pyytää tietojesi poistamista"
        },
        "extra": "Peruuttaa suostumus tietojen käyttöön"
      },
      "section6": {
        "title": "6. Tietojen säilytys",
        "content": "Säilytämme tietojasi vain niin kauan kuin on tarpeen:",
        "list": {
          "item1": "Täyttääksemme ne tarkoitukset, joita varten ne kerättiin",
          "item2": "Täyttääksemme lakisääteiset velvoitteet",
          "item3": "Ratkaistaksemme riidat"
        }
      },
      "section7": {
        "title": "7. Yhteydenotto",
        "content": "Tietosuojaan liittyvissä asioissa ota yhteyttä meihin virallisilla yhteykanavilla, jotka löytyvät alustaltamme."
      }
    },
